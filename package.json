{
	"name": "delegate-it",
	"description": "Lightweight event delegation",
	"version": "1.0.1",
	"repository": "bfred-it/delegate-it",
	"license": "MIT",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"event",
		"delegate",
		"delegation"
	],
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^1.4.2",
		"ava": "^1.3.1",
		"eslint-config-xo-typescript": "^0.8.0",
		"jsdom": "^13.2.0",
		"sinon": "^1.17.6",
		"typescript": "^3.3.3333",
		"xo": "*"
	},
	"scripts": {
		"test": "xo !index.js && ava && tsc",
		"prepublish": "tsc",
		"build": "tsc"
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"envs": [
			"browser"
		],
		"rules": {
			"prefer-rest-params": 0,
			"valid-jsdoc": 0,
			"max-params": 0
		}
	}
}
